<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vimal.apphelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppHelper">
        <activity android:name=".video.VideoActivity"></activity>
        <activity android:name=".Image.ImageviewActivity"></activity>
        <activity android:name=".Image.CircleImageviewActivity"></activity>
        <activity android:name=".AppUpdateActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.vimal.helpers.customgallery.activity.PickImageActivity" />
    </application>

</manifest>